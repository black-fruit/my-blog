{
    "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
    "version": 2,
    "builds": [
      {
        "src": "api/index.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.php"
      }
    ]
  }
